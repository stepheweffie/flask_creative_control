{
	"redis_host":	"$REDIS_HOST",

	"redis_port":	"$REDIS_PORT",
	"redis_auth":	"$REDIS_AUTH",

	"http_host":	"$HTTP_HOST",
	"http_port":	"$HTTP_PORT",

	"threads":	5,
	"pool_size": 20,

	"daemonize":	true,
	"websockets":	true,

	"database":	0,

	"acl": [
		{
			"disabled":	["DEBUG"]
		},

		{
			"http_basic_auth":	"user:password",
			"enabled":		["DEBUG"]
		}
	],

	"verbosity": 6,
	"logfile": "webdis.log"
}
